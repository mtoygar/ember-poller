{
  "name": "ember-poller",
  "version": "1.0.0",
  "description": "A poller service based on ember-concurrency",
  "keywords": [
    "ember-addon",
    "polling",
    "poll",
    "concurrency",
    "task"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mtoygar/ember-poller.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
